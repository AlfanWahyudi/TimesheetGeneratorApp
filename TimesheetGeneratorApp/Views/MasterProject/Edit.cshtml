@model TimesheetGeneratorApp.Models.MasterProjectModel

@{
    Layout = "~/Views/Shared/_template_admin.cshtml";
    ViewData["menu_master_project"] = "active";
}
<h1 class="mb-4">
    <a class="btn btn-info" asp-action="Index"><i class="fa fa-chevron-left" aria-hidden="true"></i></a>
    Master Project - Edit</h1>

<div class="card">
    <div class="card-body">
        <div class="card-title mb-3">
            <h4>From Edit</h4>
        </div>
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <partial name="_form" />
            <div class="form-group text-end mb-3">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>

</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
