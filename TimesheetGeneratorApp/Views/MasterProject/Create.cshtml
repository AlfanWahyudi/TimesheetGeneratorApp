@{
    Layout = "~/Views/Shared/_template_admin.cshtml";
    ViewData["menu_master_project"] = "active";
}

<h1 class="mb-4">
    <a class="btn btn-info" asp-action="Index"><i class="fa fa-chevron-left" aria-hidden="true"></i></a>
    Master Project - Create</h1>
<div class="card">
    <div class="card-body">
        <div class="card-title mb-3">Form</div>
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <partial name="_form" />
            <div class="form-group mb-3 text-end">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
